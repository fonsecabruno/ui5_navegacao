{
  "_version": "",
  "sap.app": {
    "id": "ovly.ui5navegacao",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "",
    "description": "",
    "resources": "resources.json",
    "ach": "ach",
    "dataSources": {
      "mainService": {
        "uri": "/V2/(S(brunofonseca))/OData/OData.svc/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/metadata.xml"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "sap-icon://task",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "rootView": {
      "viewName": "ovly.ui5navegacao.view.Root",
      "type": "XML",
      "async": true,
      "id": "Root"
    },
    "dependencies": {
      "minUI5Version": "1.66.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ovly.ui5navegacao.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "ovly.ui5navegacao.view",
        "controlAggregation": "masterPages",
        "controlId": "split-app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "detalhe",
          "pattern": "categoria/{id}",
          "target": [
            "TargetCategoria",
             "TargetProdutos"
          ]
        },
        {
          "name": "novaCategoria",
          "pattern": "new",
          "target": [
            "TargetNovaCategoria"
          ]
        },
        {
          "name": "padrao",
          "pattern": "",
          "target": [
            
            "TargetEmpty",
            "TargetCategoria"
          ]
        }
      ],
      "targets": {
        "TargetCategoria": {
          "viewName": "Categoria"
        },
        "TargetNovaCategoria": {
          "viewName": "NovaCategoria",
          "controlAggregation": "detailPages"
        },
        "TargetEmpty": {
          "viewName": "Empty",
          "controlAggregation": "detailPages"
        },
        "TargetProdutos": {
          "viewName": "Produtos",
          "controlAggregation": "detailPages"
        }
      }
    }
  }
}